# Example Terraform variables for state backend module
# Copy this file to terraform.tfvars and update with your values

# Name of the S3 bucket for Terraform state storage
# Must be globally unique across all AWS accounts
# Recommended format: <project>-terraform-state-<environment>
state_bucket_name = "bedrock-chat-poc-terraform-state-dev"

# Tags to apply to all resources
tags = {
  Environment = "dev"
  Project     = "bedrock-chat-poc"
  ManagedBy   = "Terraform"
  CreatedAt   = "2025-12-07"
}
